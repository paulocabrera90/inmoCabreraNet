@model inmoCabreraNet.Models.Inquilino

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del Inquilino</h1>

<div>
    <h4>Inquilino @Html.DisplayFor(model => model.inq_nombre)</h4>
    <hr />
    <dl class="row">
       
        <dt class = "col-sm-2">
            DNI
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.inq_dni)
        </dd>
        <dt class = "col-sm-2">
            Nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.inq_nombre)
        </dd>
        <dt class = "col-sm-2">
            Fec. de Nacimiento
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.inq_fechanac)
        </dd>
        <dt class = "col-sm-2">
            Domicilio Trabajo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.inq_domicilioTrabajo)
        </dd>
        <dt class = "col-sm-2">
            Telefono
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.inq_telef)
        </dd>
        <dt class = "col-sm-2">
            Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.inq_email)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { id= Model.inq_id }) |
    <a asp-action="Index">Back to List</a>
</div>
