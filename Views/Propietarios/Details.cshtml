@model inmoCabreraNet.Models.Propietario

@{
    ViewData["Title"] = "Detalles Propietario";
}

<h1>Detalles del Propietario</h1>

<div>
    <h4>Propietario</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
           DNI 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.pro_dni)
        </dd>
        <dt class = "col-sm-2">
           Nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.pro_nombre)
        </dd>
        <dt class = "col-sm-2">
           Fecha Nacimiento
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.pro_fechanac)
        </dd>
        <dt class = "col-sm-2">
            Dirección
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.pro_direc)
        </dd>
        <dt class = "col-sm-2">
           Teléfono
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.pro_telef)
        </dd>
        <dt class = "col-sm-2">
          Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.pro_email)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new {  id = Model.pro_id  }) |
    <a asp-action="Index">Back to List</a>
</div>
